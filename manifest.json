{
    "manifest_version": 3,
    "name": "Empty Cache & Hard Reload",
    "version": "1.0",
    "description": "Perform an empty cache and hard reload of the current site.",
    "permissions": ["activeTab", "browsingData"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  